        <style>
            .fullpage-container {
                background-color: #000;
                perspective: 1000px;
            }

            .project-section {
                min-height: 100vh;
                position: relative;
                display: flex;
                align-items: center;
                justify-content: center;
                overflow: hidden;
                padding: 4rem 0;
            }

            .project-background {
                position: absolute;
                top: 0;
                left: 0;
                width: 100%;
                height: 100%;
                background-size: cover;
                background-position: center;
                filter: brightness(0.3);
                transform: scale(1.1);
                transition: all 1.2s cubic-bezier(0.4, 0, 0.2, 1);
                box-shadow: inset 0 0 100px rgba(0,0,0,0.8);
            }

            .project-section.active .project-background {
                transform: scale(1);
            }

            .project-content {
                position: relative;
                z-index: 1;
                max-width: 800px;
                margin: 0 auto;
                padding: 2rem;
                opacity: 0;
                transform: translateY(30px);
                transition: all 0.8s ease;
            }

            .project-section.active .project-content {
                opacity: 1;
                transform: translateY(0);
            }

            .project-details {
                color: #fff;
                text-align: center;
            }

            .project-title {
                font-size: 4rem;
                margin-bottom: 1.5rem;
                font-weight: 700;
                text-transform: uppercase;
                letter-spacing: 2px;
                opacity: 0;
                transform: translateY(20px);
                transition: all 0.8s ease 0.2s;
            }

            .project-section.active .project-title {
                opacity: 1;
                transform: translateY(0);
            }

            .project-description {
                font-size: 1.4rem;
                margin-bottom: 2rem;
                line-height: 1.6;
                opacity: 0;
                transform: translateY(20px);
                transition: all 0.8s ease 0.4s;
                word-wrap: break-word;
                overflow-wrap: break-word;
                white-space: normal;
            }

            .project-section.active .project-description {
                opacity: 0.9;
                transform: translateY(0);
            }

            .project-tags {
                display: flex;
                justify-content: center;
                gap: 1rem;
                margin-bottom: 2rem;
                opacity: 0;
                transform: translateY(20px);
                transition: all 0.8s ease 0.6s;
            }

            .project-section.active .project-tags {
                opacity: 1;
                transform: translateY(0);
            }

            .tag {
                background: rgba(255, 255, 255, 0.1);
                padding: 0.5rem 1rem;
                border-radius: 20px;
                font-size: 0.9rem;
                transition: all 0.3s ease;
                backdrop-filter: blur(5px);
            }

            .tag:hover {
                background: rgba(255, 255, 255, 0.2);
                transform: translateY(-2px);
            }

            .project-link {
                display: inline-block;
                background: transparent;
                color: #fff;
                padding: 1rem 2rem;
                border: 2px solid rgba(255,255,255,0.3);
                border-radius: 30px;
                text-decoration: none;
                font-weight: 600;
                transition: all 0.5s cubic-bezier(0.4, 0, 0.2, 1);
                opacity: 0;
                transform: translateY(20px);
                transition: all 0.8s ease 0.8s;
                position: relative;
                overflow: hidden;
                backdrop-filter: blur(5px);
            }

            .project-section.active .project-link {
                opacity: 1;
                transform: translateY(0);
            }

            .project-link:hover {
                background: rgba(255,255,255,0.1);
                border-color: rgba(255,255,255,0.8);
                transform: translateY(-2px);
                box-shadow: 0 0 20px rgba(255,255,255,0.2);
            }

            .project-link::before {
                content: '';
                position: absolute;
                top: 50%;
                left: 50%;
                width: 200%;
                height: 200%;
                background: rgba(255,255,255,0.1);
                transform: translate(-50%, -50%) rotate(45deg) scale(0);
                transition: transform 0.6s ease;
            }

            .project-link:hover::before {
                transform: translate(-50%, -50%) rotate(45deg) scale(1);
            }

